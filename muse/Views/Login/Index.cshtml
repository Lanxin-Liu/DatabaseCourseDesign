    <div class="js-login-area d-flex align-items-center  text-center">
        <div class="container">
            <div class="row">
                <div class="col-lg-4 offset-lg-4 p-0">
                    <div class="js-login-content">
                        @using (Html.BeginForm("Index", "Login", FormMethod.Post))
                        {
                        <form role="form" action="" method="post" class="">
                            <label>USERID <span class="primary-color">*</span></label>
                            @*<input class="input" placeholder="userid">*@

                            @Html.TextBoxFor(m => m.account, new { @class = "input", @id = "user", @required = true })
                            <label>Password<span class="primary-color">*</span></label>
                            @*<input type="Password" placeholder="**********">*@
                            @Html.PasswordFor(m => m.password, new { @class = "", @id = "password" })
                            <ul class="js-check">
                                <li>
                                    @ViewBag.Error
                                    @*<span id="loginfail" style="color:crimson">@ViewBag.Error</span>*@
                                </li>
                            </ul>
                            <div class="group">
                                <input type="submit" class="btn btn-primary" value="登录" />
                            </div>

                        </form>


                        }

                        <div class="js-create-account text-center">
                            @Html.ActionLink("create an account", "Create", new { })
                        </div>

                    </div>
                </div><!-- col-lg-4 -->
            </div><!-- row -->
        </div><!-- container -->
    </div><!-- js-login-area -->

